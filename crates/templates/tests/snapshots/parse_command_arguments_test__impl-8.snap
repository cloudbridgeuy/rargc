---
source: crates/templates/tests/parse_command_arguments_test.rs
description: Parse unnamed commands with a default value
expression: output
---

  action="${1:-}"

  case $action in

    bar)
      action="bar"
      input=("${input[@]:1}")
      ;;
    baz)
      action="baz"
      input=("${input[@]:1}")
      ;;
    foo)
      action="foo"
      input=("${input[@]:1}")
      ;;
    -h | --help)
      usage
      exit
      ;;
    "")
      action="baz"
      ;;
    *)
      action="baz"
      ;;
  esac

