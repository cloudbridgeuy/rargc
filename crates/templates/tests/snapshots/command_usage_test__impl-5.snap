foo_usage() {
  printf "Test simple usage\n"
  printf "\n\033[4m%s\033[0m\n" "Usage:"
  printf "  foo [OPTIONS]\n"
  printf "  foo -h|--help\n"

  printf "\n\033[4m%s\033[0m\n" "Options:"
  printf "  -d --default [<DEFAULT>]\n"
  printf "    Test default option\n"
  printf "    [@default foo]\n"
  printf "  -h --help\n"
  printf "    Print help\n"
}

