run() {
  declare -A args=()
  declare -a input=()
  normalize_input "$@"
  parse_arguments "${input[@]}"
  # Configure the default action if none was set.
  if [[ -z "$action" ]]; then
    action=foo
  fi
  
  # Call the right command action
  case "$action" in
    "bar") bar ;;
  
    "baz") baz ;;
  
    "foo") foo ;;
  esac
}

