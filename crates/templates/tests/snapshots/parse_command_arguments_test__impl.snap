---
source: crates/templates/tests/parse_command_arguments_test.rs
expression: output
---
action="${1:-}"

case $action in

    bar)
      action="bar"
      shift
      ;;
    baz)
      action="baz"
      shift
      ;;
    foo)
      action="foo"
      shift
      ;;
    -h | --help)
      usage
      exit
      ;;
    "")
      usage >&2
      exit 1
      ;;
    *)
      printf "Invalid command: %s\n" "$action" >&2
      exit 1
      ;;
  esac

