run() {
  declare -A args=()
  declare -A deps=()
  declare -a input=()
  normalize_input "$@"
  parse_arguments "${input[@]}"
  # Check global environment variables
  
  if [[ -n "${FOO:-}" ]]; then
    args['foo']="${FOO:-}"
  fi

}

